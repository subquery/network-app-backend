{
  "name": "subquery-network-subql-project",
  "version": "0.1.0",
  "description": "Subql project for indexing the Subquery Network",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn check-filters && subql build",
    "codegen": "subql codegen",
    "publish": "subql publish",
    "validate": "subql validate",
    "prepare": "husky install",
    "postinstall": "sh ./scripts/postinstall.sh",
    "prepack": "rm -rf dist && yarn build",
    "check-filters": "node -r ts-node/register/transpile-only ./scripts/checkFilters.ts"
  },
  "homepage": "https://github.com/subquery/subquery-network-subql-project",
  "repository": "https://github.com/subquery/subquery-network-subql-project",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "SubQuery Pty. Limited",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polkadot/api": "^9",
    "@subql/cli": "^1.6.0",
    "@subql/types": "^1.3.0",
    "@types/bs58": "^4.0.1",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "4",
    "eslint": "7",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": ">=6",
    "js-yaml": "^4.1.0",
    "lint-staged": ">=10",
    "moonbeam-types-bundle": "^2.0.8",
    "prettier": "2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@subql/contract-sdk": "^0.11.1-8",
    "@subql/frontier-evm-processor": "^0.1.3-0",
    "@types/pino": "^7.0.5",
    "@types/validator": "^13.7.1",
    "bs58": "^4.0.1",
    "ethers": "^5.7.2",
    "node-fetch": "^2.6.7",
    "pino": "^7.8.0"
  },
  "resolutions": {
    "class-transformer": "0.4.0",
    "ipfs-unixfs": "6.0.6",
    "@polkadot/util": "10.1.5",
    "@ethersproject/base64": "5.7.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "exports": {
    "frontierEvm": "./node_modules/@subql/frontier-evm-processor/dist/index.js",
    "chaintypes": "./src/chaintypes.ts"
  }
}
