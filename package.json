{
  "name": "subquery-network-subql-project",
  "version": "0.1.0",
  "description": "Subql project for indexing the Subquery Network",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn check-filters && npx subql build",
    "prepack": "rm -rf dist && npm build",
    "test": "jest",
    "codegen": "./node_modules/.bin/subql codegen",
    "prepare": "husky install",
    "check-filters": "node -r ts-node/register/transpile-only ./scripts/checkFilters.ts"
  },
  "homepage": "https://github.com/subquery/subquery-network-subql-project",
  "repository": "https://github.com/subquery/subquery-network-subql-project",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "SubQuery Pty. Limited",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polkadot/api": "^8",
    "@subql/cli": "^1.0.0",
    "@subql/types": "^1.0.1-0",
    "@types/bs58": "^4.0.1",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "4",
    "eslint": "7",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": ">=6",
    "js-yaml": "^4.1.0",
    "lint-staged": ">=10",
    "moonbeam-types-bundle": "^2.0.8",
    "prettier": "2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@subql/contract-processors": "^0.6.0",
    "@subql/contract-sdk": "0.10.2-5",
    "@types/pino": "^7.0.5",
    "@types/validator": "^13.7.1",
    "bs58": "^4.0.1",
    "pino": "^7.8.0"
  },
  "resolutions": {
    "class-transformer": "0.4.0",
    "ipfs-unixfs": "6.0.6"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "exports": {
    "chaintypes": "./chaintypes.js"
  }
}
